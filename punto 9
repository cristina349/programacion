/*
Hacer un programa para ingresar tres números y listar el máximo de ellos
*/

#include <iostream>
using namespace std;
int main (){
    int n1, n2, n3, maxi;
    cout << "Ingrese tres numeros: "<< endl;
    cin >> n1 >> n2 >> n3;
    maxi = n1;
    if (n2 > maxi) {maxi = n2;} if(n3 > maxi) {maxi = n3;}
    cout << "El maximo es: " << maxi << endl;
  
  
}
